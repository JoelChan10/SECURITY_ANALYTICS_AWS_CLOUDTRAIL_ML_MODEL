{
  "time_range_days": 30,
  "max_events_per_request": 50,
  "batch_size": 2000,
  "threat_threshold": 0.85,
  "aws_region": "us-east-1",
  "output_dir": "threat_analysis_output",
  "event_sources": [
    "iam.amazonaws.com",
    "sts.amazonaws.com",
    "ec2.amazonaws.com",
    "s3.amazonaws.com",
    "kms.amazonaws.com"
